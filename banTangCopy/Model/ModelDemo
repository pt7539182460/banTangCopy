//数据库内创建了新表ban_tang_copy
CREAT%20TABLE%20ban_tang_copy(user_name%20varchar(50),%20password%20varchar(50))

//字段1
user_name
varchar(50)

//字段2
password
varchar(50)



//插入数据
INSERT%20INTO%20ban_tang_copy%20VALUES('11111111111111111111111111111111','11111111111111111111111111111111')
